{
  "expo": {
    "name": "esp32-app",
    "slug": "esp32-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/gear.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gvt3ch.esp32app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "READ_CONTACTS",
        "READ_PHONE_STATE",
        "RECORD_AUDIO",
        "CAMERA"
      ],
      "package": "com.gvt3ch.esp32app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "8edce0e1-283f-44e2-b465-91a96cd58deb"
      }
    },
    "owner": "gvt3ch"
  }
}
